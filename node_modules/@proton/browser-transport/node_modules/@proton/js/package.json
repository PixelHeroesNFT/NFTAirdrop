{
  "name": "@proton/js",
  "version": "22.0.64",
  "description": "Javascript Interface to the Proton Blockchain",
  "module": "dist/index.js",
  "author": "jafri",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "cypress": "./scripts/run-cypress.sh",
    "cypress-ui": "./scripts/run-cypress.sh ui",
    "test-node": "jest src/tests/*node*",
    "test-all": "yarn test && yarn test-node && yarn cypress",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "jest src/tests/*eosjs*",
    "testwallet": "jest src/tests/*wallet*",
    "prepare": "npm run build",
    "build-web": "webpack --config webpack.prod.js && webpack --config webpack.debug.js",
    "build-production": "yarn build && yarn build-web && yarn test-all",
    "clean": "rm -rf dist",
    "docs-init": "sh .docs/scripts/init.sh",
    "docs-build": "sh .docs/scripts/build.sh",
    "docs-serve": "python -m SimpleHTTPServer",
    "docs-publish": "sh .docs/scripts/publish.sh"
  },
  "dependencies": {
    "@bloks/constants": "^22.0.58",
    "bs58": "^4.0.1",
    "cross-fetch": "^3.1.4",
    "elliptic": "^6.5.4",
    "fast-text-encoding": "^1.0.3",
    "hash.js": "1.1.7",
    "pako": "2.0.3",
    "ripemd-ts": "0.0.2",
    "zod": "^1.11.13"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@blockone/eslint-config-blockone": "^3.0.0",
    "@types/bs58": "^4.0.0",
    "@types/debug": "^4.1.5",
    "@types/elliptic": "^6.4.12",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/pako": "^1.0.1",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "cypress": "^7.1.0",
    "debug": "^4.3.1",
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.1.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0"
  },
  "resolutions": {
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./src/tests/setupJest.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(tsx?)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "diagnostics": false
      }
    },
    "testRegex": "(/src/.*(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node"
  }
}
